{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "components": [
        "./components"
      ]
    },
    "target": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "jsx": "preserve",
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true
  },
  "include": [
    "src"
  ]
}
